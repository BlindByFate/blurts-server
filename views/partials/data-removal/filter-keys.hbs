<div class="remove-filter-key flx flx-grow">
  <ul class="remove-filter-key-list flx">
    {{#getRemovalFilters}}
      {{#each this.filters}}
        <li class="remove-filter-key-list-item flx jst-cntr" data-id="{{this.id}}" role="button">
            <img
              class="remove-filter-key-list-icon"
              src="{{this.icon}}"
              alt="{{this.statusText}} icon"
            />
            <span
              class="remove-filter-key-list-title"
            >{{this.statusText}}</span>
        </li>
      {{/each}}
    {{/getRemovalFilters}}
  </ul>
</div>